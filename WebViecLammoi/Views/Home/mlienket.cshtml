@using WebViecLammoi.Models
<div class="body" style="padding-bottom:0">
    <style>
        .select_t {
            border-radius: 0;
            border: 0;
            border-bottom: 2px solid rgb(0,111,199);
            box-shadow: none;
        }

        .div_websitetinh p {
            margin: 0;
            padding: 20px;
        }

            .div_websitetinh p img {
                width: 80%;
                height: 200px;
                border-radius: 50%;
                border: 1px solid #ccc;
                padding: 3px;
            }
    </style>

    <div class="row div_websitetinh">
        <div class="col-md-4 ">
            <p>

                <img class="lazy" data-src="data:image/jpeg;base64," />
            </p>
            <select class="form-control select_t" id="Select_LienKetWebSite" onchange="LienKetWebSite_OnChange(this);" style="width:100%">
                <option value="" style="text-align:center;">Website sở, ban, ngành</option>
                @foreach (m_Lienket gt in ViewBag.Lienket1)
                {
                    <option value="@gt.link">@gt.Tenweb</option>
                }
            </select>
            <script type="text/javascript" xmlns:rs="urn:schemas-microsoft-com:rowset" xmlns:z="#RowsetSchema" xmlns:cs="urn:my-scripts">
                function LienKetWebSite_OnChange(element) {
                    var m_Url = element.value;
                    if (m_Url != '') {
                        window.open(m_Url, "_blank");
                    }
                }
            </script>

        </div>
        <div class="col-md-4">
            <p>
                <img class="lazy" data-src="/Content/Tien/ban-do-tphcm.jpg" style="  " />
            </p>
            <select class="form-control select_t" id="Select_LienKetWebSite" onchange="LienKetWebSite_OnChange(this);" style="width:100%">
