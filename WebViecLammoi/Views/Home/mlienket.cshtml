                }
            </select>
            <script type="text/javascript" xmlns:rs="urn:schemas-microsoft-com:rowset" xmlns:z="#RowsetSchema" xmlns:cs="urn:my-scripts">
                function LienKetWebSite_OnChange(element) {
                    var m_Url = element.value;
                    if (m_Url != '') {
                        window.open(m_Url, "_blank");
                    }
                }
            </script>

        </div>
        <div class="col-md-4 ">
            <p>
                <img class="lazy" data-src="/Content/Tien/logobonhandienthuonghieucompress_1686133277.png" />

            </p>
            <select class="form-control select_t" id="Select_LienKetWebSite" onchange="LienKetWebSite_OnChange(this);" style="width:100%">
                <option value="" style="text-align:center;">Website cơ quan đơn vị trong tỉnh</option>
