@*<!DOCTYPE html>*@
<html>
@{

}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Trung tâm Dịch vụ Việc làm Việt Nam" />
    <meta name="geo.region" content="VN-DVVL" />
    <meta name="geo.placename" content="Việt Nam" />
    <meta name="robots" content="index, archive, follow, noodp">
    <meta name="googlebot" content="index,archive,follow,noodp">
    <meta name="msnbot" content="all,index,follow">
    <meta name="keywords" content="việc làm, lao động, tuyển dụng, doanh nghiệp" />
    <meta name="keywords" content="viec-lam,lao-dong,tuyen-dung,doanh-nghiep" />
    <meta property="og:title" content="Trung tâm Dịch vụ Việc làm Việt Nam">
    @RenderSection("metas", required: false)
    <title>Trung Tâm DVVL Ninh Thuận</title>
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/AdminBSB-css")


    <link href="~/Content/font-awesome/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">

    <!-- JQuery DataTable Css -->
    <link href="~/Content/AdminBSB-cssjs/plugins/jquery-datatable/skin/bootstrap/css/dataTables.bootstrap.css" rel="stylesheet">
    <!-- Light Gallery Plugin Css -->
    <link href="~/Content/AdminBSB-cssjs/plugins/light-gallery/css/lightgallery.css" rel="stylesheet">

    <!-- Waves Effect Css -->
    <link href="~/Content/AdminBSB-cssjs/plugins/node-waves/waves.css" rel="stylesheet" />
    <link href="~/Content/Tien/css_t.css" rel="stylesheet" />
    <link href="~/Content/owlcarousel/assets/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/owlcarousel/assets/owl.theme.default.css" rel="stylesheet" />
    <link href="~/Content/Tien/CardCss.css" rel="stylesheet" />

    <link href="~/Content/Tien/Tien-Style.css" rel="stylesheet" />

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/AdminBSB-js")

    <style>
        .div_noidungtintuc {
            text-align: justify;
            font-size: 16px;
        }

            .div_noidungtintuc p {
                text-align: justify;
            }

            .div_noidungtintuc img {
                max-width: 100%;
                margin: auto;
                display: block;
            }

        .glyphicon {
            line-height: 2 !important;
        }
        
    </style>
    <style>
        .bgnavbarTD {
            background: rgb(0, 111, 199);
            background: -moz-linear-gradient(180deg, rgba(0, 111, 199, 1) 0%, rgba(0, 114, 181, 1) 50%, rgba(0, 111, 199, 1) 100%);
            background: -webkit-linear-gradient(180deg, rgba(0, 111, 199, 1) 0%, rgba(0, 114, 181, 1) 50%, rgba(0, 111, 199, 1) 100%);
            background: linear-gradient(180deg, rgba(0, 111, 199, 1) 0%, rgba(0, 114, 181, 1) 50%, rgba(0, 111, 199, 1) 100%);
            filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#006fc7",endColorstr="#006fc7",GradientType=1);
            margin: 0 !important;
            color: white;
            padding: 0 !important;

        }
        .bgnavbar {
            background: rgb(0,111,199);
            background: -moz-linear-gradient(180deg, rgba(0,111,199,1) 0%, rgba(0,114,181,1) 50%, rgba(0,111,199,1) 100%);
            background: -webkit-linear-gradient(180deg, rgba(0,111,199,1) 0%, rgba(0,114,181,1) 50%, rgba(0,111,199,1) 100%);
            background: linear-gradient(180deg, rgba(0,111,199,1) 0%, rgba(0,114,181,1) 50%, rgba(0,111,199,1) 100%);
            filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#006fc7",endColorstr="#006fc7",GradientType=1);
            margin: 0 !important;
            color: white;
            padding: 0 !important;
            border-bottom: 1px solid #0404aa;
        }

        .navbar-default .navbar-nav > li {
            text-align: center
        }

        .navbar-default .navbar-nav li a {
            color: white !important;
        }

        .navbar-default .navbar-brand {
            color: white !important;
        }

        .ul_menu > li {
            background: url(/Content/Tien/bg-li-mainmenu.png) no-repeat left center;
        }

        .ul_menu li a {
            font-size: 15px;
            letter-spacing: 0.5px;
            padding: 12px;
            text-transform: uppercase;
        }

        .ul_menu > li {
            width: auto;
            transition: 0.3s;
        }

        .navbar {
            min-height: 0;
        }

        .navbar-brand {
            height: auto;
        }

        .ul_menu li a:hover,
        .ul_menu li a:focus {
            font-weight: bold;
            color: #0071bc !important
        }

        .ul_menu li:hover,
        .ul_menu li:focus {
            background-color: white;
            color: #0071bc !important
        }

        .navbar-default .navbar-brand:hover,
        .navbar-default .navbar-brand:focus {
            color: white;
        }

        .ul_menu2 li {
            border-top: 1px #f8f8f82b solid;
            margin: 5px;
        }

        .title_user {
            font-style: inherit;
            font-weight:bold;
        }

        .div_nganhnghe {
            padding: 12px;
            margin-bottom: 0px !important;
            background-color: #f3f6f9;
            border-radius: 5px;
        }

            .div_nganhnghe a {
                display: block;
            }

            .div_nganhnghe:hover {
                background-color: #0f338e;
            }

            .div_nganhnghe .span_so {
                padding: 10px 15px;
                font-weight: bold;
                width: 70px;
                color: #007ad9;
                display: block;
                float: left;
                background-color: white;
                border-radius: 5px;
                text-align: center;
                font-size: 20px;
            }

            .div_nganhnghe .span_tieude {
                padding: 11px 15px;
                font-weight: bold;
                display: block;
                float: left;
                font-size: 16px;
                color: #2a2c31
            }

            .div_nganhnghe:hover .span_tieude {
                color: white
            }

        .nav-tabs {
            padding-bottom: 10px;
            margin-bottom: 5px
        }

            .nav-tabs > li.active > a, .nav-tabs > li.active > a:focus, .nav-tabs > li.active > a:hover {
                border-top: 0px;
                border-bottom: 0px
            }

        .h4_thongtin {
            background-color: #fff;
            color: #183883 !important;
            font-family: 'Times New Roman';
            font-size: 18px;
            font-weight: 400;
            float: left;
            padding-right: 20px;
            z-index: 5;
            position: relative;
        }

        .hr_thongtin {
            position: absolute;
            top: 0;
            margin-top: 20px;
            z-index: 0;
            width: 100%;
            right: 0;
            border-width: 3px;
        }
        @@media screen and (max-width:767px) {
            #topmenu {
                color: white;
                margin-left: 0 !important;
            }
            .hr_thongtin {
                position: relative;
                top: 0;
                margin-top: 20px;
                z-index: 0;
                width: 100%;
                right: 0;
                border-width: 3px;
            }
            /*////cau hinh trang News*/
            #Panel_Right {
                display: none;
            }

            .label_cardhead {
                width: 70%;
            }
            /*////cau hinh trang News*/
         }
        #topmenu {
            color: white;
            margin-left: 45px;
        }
        .container > .navbar-header{
            margin-right:0;
            margin-left:0;
        }
.row{
    margin-right:-3px !important;
    margin-left:-3px !important;
}
        #movetop {
            display: none;
            position: fixed;
            bottom: 5px;
            right: 20px;
            z-index: 99;
            font-size: 18px;
            border: none;
            outline: none;
            cursor: pointer;
            color: #26243c;
            width: 37px;
            height: 37px;
            background: #FE79A2;
            padding: 0;
            line-height: 36px;
            border-radius: 35px;
            scroll-behavior:smooth;
            transition: 0.3s ease-out;
        }
    </style>
</head>
<body style="background: url(/Content/Tien/body-bg6.png)">
    <div class="row">
        <div class="row bgnavbar">
            @Html.Action("QLLoGo", "Home")
        </div>
        <div class="navbar navbar-default bgnavbar" style="border-radius: 0px; border-left: 0px; border-right: 0px; box-shadow: 0px 0px 6px #0000004d;  ">
            <div class="container" style="padding:0">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="/Trang-Chu" style="padding: 8px 23px;font-size: 25px"><i class="icon-home"></i></a>
                </div>
                <div class="navbar-collapse collapse" id="topmenu">

                    @Html.Partial("_TopMenu")
                </div>
            </div>


        </div>
        <div class="container body-content" style="box-shadow: 0px 0px 6px #0000004d; background-color:white;padding-top:5px ">
            <style>
                .bgcardhead {
                    background-color: rgb(0,111,199);
                    background-image: url(/Content/Tien/bieutuong_w.svg),url(/Content/Tien/CardHeadImg.png);
                    background-repeat: no-repeat;
                    background-position: 0px 0px,right top;
                    background-size: 30px 30px, auto;
                }

                .label_cardhead {
                    color: white;
                    text-transform: uppercase;
                    font-weight: bold;
                    color: #fff !important;
                    margin-bottom: 0px;
                    font-size: 14px;
                }

                .card_t {
                    border: 1px solid #ddd;
                    box-shadow: none;
                    margin: 5px;
                }

                .header_t {
                    padding: 10px 10px 10px 40px !important;
                    margin: -1px;
                    border-bottom: 1px solid #bd1701 !important;
                }

                .img_avatar {
                    width: 100%;
                    border: 1px solid rgb(200, 200, 200);
                    border-radius: 3px;
                    padding: 5px
                }
                .img_sp{
                    max-height:150px;
                }
                .div_moinhat {
                    border: 1px solid rgb(200, 200, 200);
                    border-radius: 3px;
                    padding: 10px;
                    margin-bottom: 10px
                }

                .h4_tieude {
                    color: var(--d-1,#2a2c31);
                    font-family: 'Times New Roman';
                    font-size: 16px;
                    font-style: normal;
                    font-weight: bold;
                    line-height: normal;
                }

                .h5_name {
                    color: var(--cl-1,#183883);
                    font-family: 'Times New Roman';
                    font-size: 14px;
                    font-style: normal;
                    font-weight: 400;
                    line-height: normal;
                }
            </style>
            <div class="row">
                <div class="col-md-12" style="padding-bottom: 20px; min-height: 575px">
                    @RenderBody()
                </div>

            </div>
        </div>
        
        @Html.Partial("_Footer")
    </div>
    
    <!-- Jquery DataTable Plugin Js -->
    <script src="~/Content/AdminBSB-cssjs/plugins/jquery-datatable/jquery.dataTables.js"></script>
    <script src="~/Content/AdminBSB-cssjs/plugins/jquery-datatable/skin/bootstrap/js/dataTables.bootstrap.js"></script>
    <script src="~/Content/AdminBSB-cssjs/plugins/jquery-datatable/extensions/export/dataTables.buttons.min.js"></script>
    <!-- Light Gallery Plugin Js -->
    <script src="~/Content/AdminBSB-cssjs/plugins/light-gallery/js/lightgallery-all.js"></script>
    <!-- Waves Effect Plugin Js -->
    <script src="~/Content/AdminBSB-cssjs/plugins/node-waves/waves.js"></script>
    <script src="~/Content/owlcarousel/owl.carousel.js"></script>
    <script src="~/Content/Tien/number_slideshow.js"></script>
    <script src="~/Content/AdminBSB-cssjs/js/pages/cards/basic.js"></script>

    @RenderSection("scripts", required: false)
</body>

</html>
